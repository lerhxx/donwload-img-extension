{
    "manifest_version": 2,
    "name": "download-img-extension",
    "version": "1.0.0",
    "description": "a project name download-img-extension",
    "permissions": [],
    "browser_action": {
        "default_title": "",
        "default_icon": "./images/default.png",
        "default_popup": ""
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["./lib/index.js"]
    }],
    "background": {
        "scripts": ["./src/background.js"],
        "persistent": false
    }
}